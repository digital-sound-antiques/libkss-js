{
  "name": "libkss-js",
  "version": "1.3.4",
  "description": "KSS player library for Javascript",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "files": [
    "README.md",
    "package.json",
    "LICENSE",
    "src",
    "lib"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "npm run clean && npm run build",
    "clean": "rm-cli -ss -r lib build",
    "build": "mkdirp lib build && cd build && emcmake cmake .. && cmake --build . && cpy libkss.js ../lib/",
    "jsdoc": "rm-cli -r docs ; jsdoc -c jsdoc.config.json --readme README.md",
    "lint": "eslint src"
  },
  "keywords": [],
  "author": "Mitsutaka Okazaki",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^3.6.3",
    "mkdirp": "^0.5.1",
    "prettier": "^1.18.2",
    "rm-cli": "^1.4.2",
    "wavefile": "^11.0.0"
  },
  "dependencies": {
    "cpy-cli": "^3.1.1",
    "encoding-japanese": "^1.0.26",
    "node-fetch": "^2.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/digital-sound-antiques/libkss-js.git"
  },
  "engines": {
    "node": ">=10"
  }
}
